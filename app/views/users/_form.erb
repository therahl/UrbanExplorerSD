<%= form_for @user do |f| %>

    <%= f.label :given_name %>
    <%= f.text_field :family_name %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :email%>
    <%= f.text_field :email %>

    <%= f.submit 'Submit' %>

<% end %>