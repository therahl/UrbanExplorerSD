<%= form_for @user do |f| %>
<p>
  <%= f.label :given_name %>
  <%= f.text_field :given_name %>
</p>
<p>
  <%= f.label :family_name %>
  <%= f.text_field :family_name %>
</p>

<p>
  <%= f.label :city %>
  <%= f.text_field :city %>
</p>
<p>
  <%= f.label :state %>
  <%= f.text_field :state %>
</p>

<p>
  <%= f.label :avatar %>
  <%= f.file_field :avatar %>
</p>

    <%= f.submit 'Submit' %>

<% end %>
