<%= form_for @business do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.submit 'Submit' %>

<% end %>